{
    "compilerOptions": {
      "baseUrl": ".", 
      "paths": {
        "@core/*": ["core/*"],
        "@core/set-up/*": ["core/set-up/*"], 
        "@core/configuration/*": ["core/configuration/*"], 
        "@pages/*": ["page_objects/*"]
      },
      "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
     "module": "commonjs",
    "esModuleInterop": true,
    "target": "ES6",
    "outDir": "./dist",
    "types": ["mocha", "node", "chai"]
    },
    "include": [
    "core/**/*",
    "tests/*.ts",
    "src/**/*.ts", "page_objects/baseInterface/collection.ts", "page_objects/baseInterface/basePage.ts", "tests/playwrightTests/CheckTabs.spec.ts", "tests/playwrightTests/AddNewDashboard.spec.ts" ],
  "exclude": ["node_modules"]
  }
  
